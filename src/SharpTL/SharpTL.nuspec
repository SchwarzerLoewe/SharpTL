<?xml version="1.0"?>

<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SharpTL</id>
    <version>$version$</version>
    <title>SharpTL</title>
    <authors>Alexander Logger</authors>
    <owners></owners>
    <description>
      Portable class library allows to serialize/deserialize objects defined in Type Language (https://core.telegram.org/mtproto/TL) schemas.
      Supported platforms:

      .NET Framework 4.5
      .NET for Windows Store apps
      .NET for Windows Phone 8 apps
      Portable Class Libraries
    </description>
    <releaseNotes>
    </releaseNotes>
    <summary>
      The Type Language schema converter and serializer.
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/inTagger/SharpTL</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/inTagger/SharpTL/blob/master/LICENSE.md</licenseUrl>
    <references></references>
    <dependencies>
      <dependency id="BigMath" version="0.4.1" />
    </dependencies>
    <tags>TypeLanguage TL Serialization MTProto Telegram</tags>
  </metadata>
  <files>
    <file src="SharpTL.PCL\bin\$config$\SharpTL.dll" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpTL.PCL\bin\$config$\SharpTL.pdb" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpTL.PCL\bin\$config$\SharpTL.XML" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpTL.PCL\**\*.cs" target="src\SharpTL.PCL\" exclude="SharpTL.PCL\obj\**" />
  </files>
</package>
